{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "autoprefixer": "^8.1.0",
    "gatsby": "^1.9.236",
    "gatsby-link": "^1.6.39",
    "gatsby-plugin-react-helmet": "^2.0.8",
    "gatsby-plugin-react-next": "^1.0.11",
    "postcss-cli": "^5.0.0",
    "react-helmet": "^5.2.0",
    "tailwindcss": "^0.5.1"
  },
  "keywords": ["gatsby"],
  "license": "MIT",
  "scripts": {
    "build:css": "postcss src/layouts/index.css -o src/layouts/generated.css",
    "watch:css":
      "postcss src/layouts/index.css -o src/layouts/generated.css -w",
    "build": "npm run build:css && gatsby build",
    "dev": "npm run watch:css & gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/react": "^16.0.40",
    "gatsby-plugin-typescript": "^1.4.18",
    "prettier": "^1.11.1",
    "typescript": "^2.7.2"
  }
}
